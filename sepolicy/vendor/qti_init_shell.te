#============= qti_init_shell ==============
allow qti_init_shell sysfs_cpu_boost:file write;
allow qti_init_shell sysfs_lowmemorykiller:dir write;
allow qti_init_shell debug_prop:property_service set;
allow qti_init_shell default_prop:property_service set;
allow qti_init_shell radio_prop:property_service set;
allow qti_init_shell self:capability dac_override;
allow qti_init_shell system_prop:property_service set;
allow qti_init_shell system_radio_prop:property_service set;
allow qti_init_shell sysfs:file write;
allow qti_init_shell default_prop:file { getattr open read };
allow qti_init_shell toolbox_exec:file { getattr read open execute execute_no_trans };
allow qti_init_shell vendor_data_file:file { create getattr open read write };
allow qti_init_shell exported_system_radio_prop:property_service { set };
allow qti_init_shell exported_system_radio_prop:file { open read getattr };
allow qti_init_shell vendor_data_file:dir { write read add_name };